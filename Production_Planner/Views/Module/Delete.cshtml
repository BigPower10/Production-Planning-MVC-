        @Html.ActionLink("Delete", "DeleteConfirmed", new { id = Model.MODCD }, new { onclick = "return confirm('Are you sure you want to delete this ?');", @class = "btn btn-sm btn-danger" })
        @Html.ActionLink("Back to List", "Index")
    </div>
</div>
